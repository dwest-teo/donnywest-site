{
  "name": "next-donnywest",
  "version": "1.0.0",
  "description": "test with next.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "author": "Donny West",
  "license": "MIT",
  "dependencies": {
    "next": "2.0.0-beta.32",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.10.0",
    "xo": "^0.17.1"
  },
  "engines": {
    "node": "7.6.0"
  },
  "alias": [
    "donnywest.com",
    "www.donnywest.com"
  ],
  "xo": {
    "envs": [
      "node",
      "es6",
      "browser"
    ],
    "extends": "xo-react",
    "space": true,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "react/jsx-closing-bracket-location": [
        2,
        "tag-aligned"
      ],
      "react/jsx-space-before-closing": [
        2,
        "always"
      ],
      "react/forbid-component-props": 0
    }
  }
}
